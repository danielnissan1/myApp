{
  "name": "app_c",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "start": "ts-node src/app.ts",
    "test": "jest --detectOpenHandles --forceExit --coverage",
    "build": "tsc",
    "clean:linux": "rm -rf dist",
    "clean:windows": "rd /q /s \"dist\" 2>nul",
    "serve-client": "serve -s build",
    "pm2start": "pm2 start dist/main.js --name yad2",
    "pm2stop": "pm2 stop yad2",
    "pm2restart": "pm2 restart yad2",
    "pm2logs": "pm2 logs yad2",
    "pm2delete": "pm2 delete yad2",
    "pm2restart:prod": "pm2 restart ecosystem.config.js --env production",
    "prod:local": "npm run clean:windows && npx cross-env NODE_ENV=production && npm run build && npm run pm2restart:prod",
    "prod": "npm run clean:linux && cross-env NODE_ENV=production && npm run build && npm run pm2restart:prod"
  },
  "license": "ISC",
